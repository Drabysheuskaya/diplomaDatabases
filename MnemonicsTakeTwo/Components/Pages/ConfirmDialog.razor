@inject IDialogService DialogService

<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.h6">@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Text" Color="Color.Primary" OnClick="() => MudDialog.Close(DialogResult.Ok(true))">Yes</MudButton>
        <MudButton Variant="Variant.Text" Color="Color.Secondary" OnClick="() => MudDialog.Close(DialogResult.Cancel())">No</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    [Parameter] public string ContentText { get; set; }
}




